ORG 0000H
MAIN:
MOV R6, #0H ;HOLDS FOR MAX NUMBER
MOV R7, #0FFH ;HOLDS FOR MIN  NUMBER 
MOV DPTR, #NUMBERS

ACALL MAXMIN

ORG 0100H



MAXMIN:
CLR A
MOVC A, @A+DPTR
JZ END

MAX:
CJNE A, 6, NEXT1
SJMP NEXTNUM

NEXT1:
JC MIN
MOV R6, A
SJMP NEXTNUM


MIN:
CJNE A, 7 , NEXT2
SJMP NEXTNUM

NEXT2:
JNC NEXTNUM
MOV R7, A

NEXTNUM:
INC DPTR
CJNE A, 0H, MAXMIN
END: RET
 





ORG 1000H
NUMBERS: DB 12,15,1,25,250,0