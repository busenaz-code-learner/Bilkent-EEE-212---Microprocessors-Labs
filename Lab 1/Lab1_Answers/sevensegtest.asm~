ORG 0000h
MOV A,#00h
MOV DPTR,#ARRAY
BACK: PUSH ACC
MOVC A,@A+DPTR
MOV P1,A
LCALL DELAY
POP ACC
INC A
CJNE A,#0Ah,BACK
CLR A
SJMP BACK

DELAY: MOV R5,#01
BACK5: MOV R6,#10
BACK6: MOV R7,#10
BACK7: DJNZ R7,BACK7
DJNZ R6,BACK6
DJNZ R5,BACK5
RET




ORG 1000h
ARRAY: DB 3Fh,06h,5Bh,4Fh,66h,6Dh,7Dh,07h,7Fh,6Fh